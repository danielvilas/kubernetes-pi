$ORIGIN local.
       3600 IN SOA serverpi.local. serverpi.local. (
                                2017042745 ; serial
                                7200       ; refresh (2 hours)
                                3600       ; retry (1 hour)
                                1209600    ; expire (2 weeks)
                                3600       ; minimum (1 hour)
                                )

serverpi. IN NS serverpi.local.
serverpi. IN MX 10 serverpi.local.
localhost  IN A 127.0.0.1
serverpi IN A 192.168.1.2
imac  IN A 192.168.1.3
nodepi1  IN A 192.168.1.11
nodepi2  IN A 192.168.1.12
nodepi3  IN A 192.168.1.13
masterpi  IN A 192.168.1.10
asus IN A 192.168.1.14
